pcm.!default {
        type plug
        slave.pcm "bs2b"
}

pcm.bs2b {
         type ladspa
         slave.pcm "plughw:DSD"
         path "/usr/lib/ladspa/"
         plugins [{
                 label bs2b
                 input {
                       controls [ 725 4.5 ]
                       }
                 }]
}
